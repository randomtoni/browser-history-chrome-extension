{
    "manifest_version": 3,
    "name": "History Viewer",
    "version": "1.0",
    "description": "Displays browser history within a date range.",
    "permissions": ["history"],
    "action": {
        "default_popup": "popup.html"
    },
    "commands": {
        "open_extension": {
            "suggested_key": {
                "default": "Ctrl+Shift+H", 
                "mac": "Command+Shift+H"
            },
            "description": "Open the History Viewer extension"
        }
    }
}